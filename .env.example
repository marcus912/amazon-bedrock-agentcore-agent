# Agent Name (must match agent configured in .bedrock_agentcore.yaml)
# This determines which agent configuration to deploy
AGENT_NAME=my_agent_dev

# Agent Configuration
AGENT_LOG_LEVEL=INFO

# Model Configuration (leave empty for Strands default: Claude Sonnet 4)
BEDROCK_MODEL_ID=
GITHUB_AGENT_MODEL_ID=
EMAIL_AGENT_MODEL_ID=

# Prompt Configuration
PROMPT_PROFILE=

# AgentCore Memory Configuration
# Memory ID for conversation persistence (optional)
BEDROCK_AGENTCORE_MEMORY_ID=

# Bedrock Knowledge Base Configuration
# The ID of your Bedrock Knowledge Base for semantic search and retrieval
KNOWLEDGE_BASE_ID=your-knowledge-base-id-here

# AWS Region where your Knowledge Base is hosted
AWS_REGION=us-west-2

# Minimum relevance score threshold (0.0-1.0) for knowledge base retrieval
# Results with scores below this threshold will be filtered out
MIN_SCORE=0.6

# Enable metadata in retrieval responses by default (true/false)
# When enabled, includes source URI, chunk ID, and data source ID
RETRIEVE_ENABLE_METADATA_DEFAULT=false

# GitHub MCP Server Configuration
GITHUB_PAT=your-github-pat-here

# AWS SES Configuration
SES_SENDER_EMAIL=noreply@yourdomain.com
SES_SENDER_NAME=Your App Name
